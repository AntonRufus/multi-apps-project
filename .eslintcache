State(null);       //win, lose, draw\r\n\r\n    useEffect(() => {\r\n        restartGame();\r\n    }, []);\r\n\r\n    // game state => choices\r\n    const handleUserChoice = (choice) => {\r\n        const chosenChoice = choices.find(c => c.id === choice);\r\n        setUserChoice(chosenChoice);\r\n\r\n        // determine the winner\r\n        if (chosenChoice.losesTo === computerChoice.id) {\r\n            // lose\r\n            setLosses(losses => losses + 1)\r\n            setGameState('lose')\r\n        } else if (computerChoice.losesTo === chosenChoice.id) {\r\n            // win\r\n            setWins(wins => wins + 1)\r\n            setGameState('win')\r\n        } else if (chosenChoice.id === chosenChoice.id) {\r\n            // draw\r\n            setDraws(draws => draws + 1)\r\n            setGameState('draw')\r\n        }\r\n    };\r\n\r\n    // render choice component  => game state\r\n    const renderComponent = (choice) => {\r\n        const Component = choice.component;     // Paper, Rock, Scissors\r\n        return <Component/>\r\n    };\r\n\r\n    // reset the game\r\n    const restartGame = () => {\r\n        setGameState(null);\r\n        const randomChoice = choices[Math.floor(Math.random() * choices.length)];\r\n        setComputerChoice(randomChoice);\r\n    };\r\n\r\n    return (\r\n        <div className={css.wrapper}>\r\n            {/* information goes here */}\r\n            <div className={css.info}>\r\n                <h2>Rock. Paper. Scissors.</h2>\r\n\r\n                {/* wins vs losses stats */}\r\n                <div className={css.wins_losses}>\r\n                    <div className={css.wins}>\r\n                        <span className={css.number}>{wins}</span>\r\n                        <span className={css.text}>{wins === 1 ? 'Win' : 'Wins'}</span>\r\n                    </div>\r\n\r\n                    <div className={css.losses}>\r\n                        <span className={css.number}>{losses}</span>\r\n                        <span className={css.text}>{losses === 1 ? 'Loss' : 'Losses'}</span>\r\n                    </div>\r\n\r\n                    <div className={css.draws}>\r\n                        <span className={css.number}>{draws}</span>\r\n                        <span className={css.text}>{draws === 1 ? 'Draw' : 'Draws'}</span>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n\r\n            {/* the popup to show win/lose/draw --- game state */}\r\n            {gameState && (\r\n                <div className={`game_state ${gameState}`}\r\n                     onClick={() => restartGame()}\r\n                >\r\n                    <div>\r\n                        <div className='game_state_content'>\r\n                            <p>{renderComponent(userChoice)}</p>\r\n                            {/*<p>You {gameState}!</p>*/}\r\n                            {gameState === 'win' && <p>Congrats! You won!</p>}\r\n                            {gameState === 'lose' && <p>Sorry! You lost!</p>}\r\n                            {gameState === 'draw' && <p>Oh! You drew!</p>}\r\n                            <p>{renderComponent(computerChoice)}</p>\r\n                        </div>\r\n                        <button>Play again</button>\r\n                    </div>\r\n                </div>\r\n            )}\r\n\r\n            {/* choices */}\r\n            <div className={css.choices}>\r\n                {/* choices captions */}\r\n                <div>You</div>\r\n                <div/>\r\n                <div>Computer</div>\r\n\r\n                {/* buttons for my choice */}\r\n                <div>\r\n                    <button className={css.rock} onClick={() => handleUserChoice(1)}>\r\n                        <Rock/>\r\n                    </button>\r\n                    <button className={css.paper} onClick={() => handleUserChoice(2)}>\r\n                        <Paper/>\r\n                    </button>\r\n                    <button className={css.scissors} onClick={() => handleUserChoice(3)}>\r\n                        <Scissors/>\r\n                    </button>\r\n                </div>\r\n\r\n                <div className={css.vs}>vs</div>\r\n\r\n                {/* show the computer's choice */}\r\n                <div>\r\n                    <button className={css.computer_choice}>?</button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    )\r\n};\r\n\r\nexport default PaperRockScissors;\r\n",["133","134"],"C:\\Dev\\Projects\\multi-apps-project\\src\\components\\authentication\\Authentication.jsx",["135","136"],"C:\\Dev\\Projects\\multi-apps-project\\src\\components\\nav_bar\\NavBar.jsx",["137"],"import React from \"react\";\r\nimport {NavCreator} from \"../creators/Creator\";\r\nimport css from \"../creators/Creator.module.css\";\r\n\r\nconst NavBar = () =>\r\n    <div>\r\n        <div className={css.arrows}>\r\n              <button className={css.arrow_up}>⬆</button>\r\n              <button className={css.arrow_down}>⬇</button>\r\n        </div>\r\n        <NavCreator     to={'/home'                     }       title={'Home page'}/>\r\n        <NavCreator     to={'/pomodoro_timer'           }       title={'Pomodoro Timer'}/>\r\n        <NavCreator     to={'/just_timer'               }       title={'Just Timer'}/>\r\n        <NavCreator     to={'/markdown_editor'          }       title={'Markdown Editor'}/>\r\n        <NavCreator     to={'/browser_tabs'             }       title={'Browser Tabs'}/>\r\n        <NavCreator     to={'/paper_rock_scissors'      }       title={'RPS  ✊ 🤚 🖖'}/>\r\n        <NavCreator     to={'/infinite_image_gallery'   }       title={'Infinite Gallery'}/>\r\n        <NavCreator     to={'/trivia'                   }       title={'Trivia'}/>\r\n        <NavCreator     to={'/authentication/home'      }       title={'Authentication'}/>\r\n        <hr/>\r\n        <div className={css.moving_boxes_link}>\r\n                <a target='_blank' href=\"htt