        if (chosenChoice.losesTo === computerChoice.id) {\r\n            // lose\r\n            setLosses(losses => losses + 1)\r\n            setGameState('lose')\r\n        } else if (computerChoice.losesTo === chosenChoice.id) {\r\n            // win\r\n            setWins(wins => wins + 1)\r\n            setGameState('win')\r\n        } else if (chosenChoice.id === chosenChoice.id) {\r\n            // draw\r\n            setDraws(draws => draws + 1)\r\n            setGameState('draw')\r\n        }\r\n    };\r\n\r\n    // render choice component  => game state\r\n    const renderComponent = (choice) => {\r\n        const Component = choice.component;     // Paper, Rock, Scissors\r\n        return <Component/>\r\n    };\r\n\r\n    // reset the game\r\n    const restartGame = () => {\r\n        setGameState(null);\r\n        const randomChoice = choices[Math.floor(Math.random() * choices.length)];\r\n        setComputerChoice(randomChoice);\r\n    };\r\n\r\n    return (\r\n        <div className={css.wrapper}>\r\n            {/* information goes here */}\r\n            <div className={css.info}>\r\n                <h2>Rock. Paper. Scissors</h2>\r\n\r\n                {/* wins vs losses stats */}\r\n                <div className={css.wins_losses}>\r\n                    <div className={css.wins}>\r\n                        <span className={css.number}>{wins}</span>\r\n                        <span className={css.text}>{wins === 1 ? 'Win' : 'Wins'}</span>\r\n                    </div>\r\n\r\n                    <div className={css.losses}>\r\n                        <span className={css.number}>{losses}</span>\r\n                        <span className={css.text}>{losses === 1 ? 'Loss' : 'Losses'}</span>\r\n                    </div>\r\n\r\n                    <div className={css.draws}>\r\n                        <span className={css.number}>{draws}</span>\r\n                        <span className={css.text}>{draws === 1 ? 'Draw' : 'Draws'}</span>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n\r\n            {/* the popup to show win/lose/draw --- game state */}\r\n            {gameState && (\r\n                <div className={`game_state ${gameState}`}\r\n                     onClick={() => restartGame()}\r\n                >\r\n                    <div>\r\n                        <div className='game_state_content'>\r\n                            <p>{renderComponent(userChoice)}</p>\r\n                            {/*<p>You {gameState}!</p>*/}\r\n                            {gameState === 'win' && <p>Congrats! You won!</p>}\r\n                            {gameState === 'lose' && <p>Sorry! You lost!</p>}\r\n                            {gameState === 'draw' && <p>Oh! You drew!</p>}\r\n                            <p>{renderComponent(computerChoice)}</p>\r\n                        </div>\r\n                        <button>Play again</button>\r\n                    </div>\r\n                </div>\r\n            )}\r\n\r\n            {/* choices */}\r\n            <div className={css.choices}>\r\n                {/* choices captions */}\r\n                <div>You</div>\r\n                <div/>\r\n                <div>Computer</div>\r\n\r\n                {/* buttons for my choice */}\r\n                <div>\r\n                    <button className={css.rock} onClick={() => handleUserChoice(1)}>\r\n                        <Rock/>\r\n                    </button>\r\n                    <button className={css.paper} onClick={() => handleUserChoice(2)}>\r\n                        <Paper/>\r\n                    </button>\r\n                    <button className={css.scissors} onClick={() => handleUserChoice(3)}>\r\n                        <Scissors/>\r\n                    </button>\r\n                </div>\r\n\r\n                <div className={css.vs}>vs</div>\r\n\r\n                {/* show the computer's choice */}\r\n                <div>\r\n                    <button className={css.computer_choice}>?</button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    )\r\n};\r\n\r\nexport default PaperRockScissors;\r\n",["121","122"],"C:\\Dev\\Projects\\multi-apps-project\\src\\components\\paper_rock_scissors\\icons\\Paper.jsx",[],"C:\\Dev\\Projects\\multi-apps-project\\src\\components\\paper_rock_scissors\\icons\\Rock.jsx",[],"C:\\Dev\\Projects\\multi-apps-project\\src\\components\\paper_rock_scissors\\icons\\Scissors.jsx",[],"C:\\Dev\\Projects\\multi-apps-project\\src\\components\\moving_boxes\\MovingBoxes.jsx",[],"C:\\Dev\\Projects\\multi-apps-project\\src\\components\\markdown_editor\\MarkdownEditor.jsx",[],"C:\\Dev\\Projects\\multi-apps-project\\src\\components\\infinite_image_gallery\\InfiniteImageGallery.jsx",[],"C:\\Dev\\Projects\\multi-apps-project\\src\\components\\trivia\\Trivia.jsx",[],{"ruleId":"123","replacedBy":"124"},{"ruleId":"125","replacedBy":"126"},{"ruleId":"123","replacedBy":"127"},{"ruleId":"125","replacedBy":"128"},{"ruleId":"123","replacedBy":"129"},{"ruleId":"125","replacedBy":"130"},{"ruleId":"123","replacedBy":"131"},{"ruleId":"125","replacedBy":"132"},{"ruleId":"123","replacedBy":"133"},{"ruleId":"125","replacedBy":"134"},{"ruleId":"135","severity":2,"message":"136","line":25,"column":50,"nodeType":"137","endLine":25,"endColumn":56},{"ruleId":"138","severity":1,"message":"139","line":44,"column":20,"nodeType":"140","messageId":"141","endLine":44,"endColumn":55},{"ruleId":"123","replacedBy":"142"},{"ruleId":"125","replacedBy":"143"},"no-native-reassign",["144"],"no-negated-in-lhs",["145"],["144"],["145"],["144"],["145"],["144"],["145"],["144"],["145"],"react/jsx-no-undef","'Trivia' is not defined.","JSXIdentifier","no-self-compare","Comparing to itself is potentially pointless.","BinaryExpression","comparingToSelf",["144"],["145"],"no-global-assign","no-unsafe-negation"]